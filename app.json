{
  "expo": {
    "name": "Gedeón",
    "description": "Esta aplicación contiene el cancionero de la Iglesia ICI Santiago",
    "slug": "gedeon",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.icisantiago.gedeon"
    },
    "android": {
      "package": "com.icisantiago.gedeon",
      "permissions": [],
      "versionCode": 4
    },
    "githubUrl": "https://github.com/ICI-Santiago/gedeon-app-mobile"
  }
}
